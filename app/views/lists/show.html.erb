<%= @list.name %>

<%= link_to 'delete this list', list_path(@list), method: :delete, data: { confirm: 'Are you sure you want to delete this list?' } %>
<%= link_to 'add a bookmark', new_list_bookmark_path(@list) %>
<%= link_to 'back to lists', lists_path(@lists) %>

<% @list.movies.each do |movie| %>
  <%= movie.title %>
<% end %>

<%= cl_image_tag @list.photo.key, height: 300, width: 400, crop: :fill %>
